//size the window
size(400, 400);

//set background color to 0(black)
background(92351465);

//set the color for the shape
fill(255,0,0);

//shape definition, must start with beginShape() and end
//with endShape(). Note the capital "S" in shape.
beginShape();

vertex(220,90);
vertex(220,130);
vertex(180,130);
vertex(180,90);
vertex(160,90);

endShape();

//Now you draw a down arrow under the up-arrow.
